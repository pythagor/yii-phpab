        "docs": "https://github.com/pythagor/yii-phpab/blob/master/docs/guide/README.md"
    },
    "authors": [
        {
            "name": "Andrei Chugunov",
            "email": "admin@pythagor.com",
            "homepage": "http://pythagor.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii": "*",
        "pythagor/php-a-b": "*"
